{
  "name": "pinpoint",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "prisma migrate deploy && next build",
    "db:generate": "prisma migrate dev",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "dev": "dotenv -- next dev --turbo",
    "dev:full": "concurrently -n 'server,db-studio,typecheck' -c 'blue,green,yellow' --kill-others 'npm run dev:server' 'npm run dev:db' 'npm run dev:typecheck'",
    "dev:clean": "node scripts/cleanup-dev.cjs && npm run dev:full",
    "dev:server": "nodemon --config nodemon.server.json",
    "dev:db": "dotenv -- sh -c 'prisma studio --port ${PRISMA_STUDIO_PORT:-5555}'",
    "dev:typecheck": "tsc --noEmit --watch --preserveWatchOutput",
    "dev:status": "concurrently 'npm run health' 'node scripts/port-status.cjs'",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "postinstall": "prisma generate",
    "lint": "NODE_ENV=development next lint",
    "lint:fix": "NODE_ENV=development next lint --fix --quiet",
    "lint:quiet": "NODE_ENV=development next lint --quiet",
    "preview": "next build && next start",
    "seed": "tsx prisma/seed.ts",
    "start": "next start",
    "test": "npm run test:unit",
    "test:unit": "jest --testPathIgnorePatterns=integration-tests",
    "test:integration": "jest --testMatch='**/integration-tests/**/*.test.ts'",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "typecheck": "tsc --noEmit",
    "typecheck:files": "fun () { npx tsc --noEmit \"$@\"; }; fun",
    "typecheck:routes": "./scripts/typecheck-grep.sh \"route\\.ts\"",
    "typecheck:tests": "./scripts/typecheck-grep.sh \"test\\.ts\"",
    "typecheck:changed": "fun () { git diff --name-only --diff-filter=AM | grep -E '\\.(ts|tsx)$' | xargs -r npx tsc --noEmit; }; fun",
    "quick:agent": "npm-run-all --print-label fix:agent typecheck:agent lint:agent",
    "fix": "npm run lint:fix && npm run format:write",
    "validate": "npm run typecheck && npm run lint && npm run format:check",
    "validate:agent": "npm-run-all --print-label fix:agent typecheck:agent lint:agent format:agent test:agent",
    "fix:agent": "npm run lint:fix --silent > /dev/null 2>&1 && npm run format:write --silent > /dev/null 2>&1 || echo '✓ Auto-fixed lint + format'",
    "typecheck:agent": "tsc --noEmit --pretty false --skipLibCheck | grep -E 'error TS|Found [0-9]+ error' | head -5 || echo '✓ TypeScript: 0 errors'",
    "lint:agent": "eslint --quiet --format compact src/ | head -5 || echo '✓ ESLint: 0 problems'",
    "format:agent": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache | grep -E 'Code style issues|would be re-written' | wc -l | xargs -I {} echo 'Prettier: {} files need formatting' || echo '✓ Prettier: All formatted'",
    "test:agent": "cross-env AGENT_MODE=true jest --silent --passWithNoTests | grep -E 'FAIL|Tests:|passed|failed' | head -3 || echo '✓ Tests: All passing'",
    "validate:full": "npm run typecheck && npm run lint && npm run format:check && npm run test:coverage && npm run workflow:lint && npm run deps:check && npm run smoke-test && npm run debug:ci",
    "validate:full:agent": "npm-run-all --print-label validate:agent workflow:agent deps:agent smoke:agent",
    "workflow:agent": "actionlint .github/workflows/*.yml 2>&1 | head -3 || echo '✓ Workflows: Valid'",
    "deps:agent": "npm audit --audit-level=high --format json 2>/dev/null | jq -r '.metadata.vulnerabilities | \"High: \\(.high), Critical: \\(.critical)\"' 2>/dev/null || echo '✓ Dependencies: No high/critical vulnerabilities'",
    "smoke:agent": "npm run build --silent 2>&1 | grep -E 'Failed to compile|Syntax error|Type error' | head -3 || echo '✓ Build: Successful'",
    "debug:typecheck": "tsc --noEmit",
    "debug:test": "jest --verbose",
    "debug:lint": "eslint src/ --format detailed",
    "debug:format": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "debug:ci": "./scripts/debug-ci-difference.sh",
    "workflow:lint": "actionlint .github/workflows/*.yml",
    "betterer": "betterer",
    "betterer:check": "betterer ci",
    "betterer:update": "betterer update",
    "betterer:watch": "betterer watch",
    "pre-commit": "npm run validate && npm run workflow:lint && npm run smoke-test",
    "smoke-test": "npm run build --silent && echo 'Build successful - basic smoke test passed'",
    "health": "node scripts/health-check.cjs",
    "kill:all": "node scripts/kill-dev-processes.cjs",
    "port:free": "node scripts/free-ports.cjs 3000 5555",
    "deps:check": "npm audit --audit-level=high",
    "deps:fix": "npm audit fix",
    "deps:update": "npm update && npm audit",
    "clean": "rm -rf .next node_modules/.cache .turbo && npm install",
    "reset": "npm run clean && npm run db:push && npm run seed",
    "setup:worktree": "bash scripts/setup-worktree.sh",
    "db:reset": "rm -rf prisma/migrations && npx prisma db push --force-reset && npm run seed",
    "pinballmap:update-fixture": "curl -o src/lib/pinballmap/__tests__/fixtures/api_responses/locations/location_26454_machine_details.json https://pinballmap.com/api/v1/locations/26454/machine_details.json",
    "prepare": "husky"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "7.2.0",
    "@mui/material": "7.2.0",
    "@prisma/client": "^6.12.0",
    "@prisma/extension-accelerate": "^2.0.2",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@tanstack/react-query": "^5.83.0",
    "@trpc/client": "^11.0.0",
    "@trpc/react-query": "^11.0.0",
    "@trpc/server": "^11.0.0",
    "@types/qrcode": "^1.5.5",
    "actionlint": "^2.0.6",
    "csv-parser": "^3.2.0",
    "next": "15.4.2",
    "next-auth": "5.0.0-beta.29",
    "prisma": "^6.12.0",
    "qrcode": "^1.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "server-only": "^0.0.1",
    "superjson": "^2.2.1",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@betterer/cli": "^6.0.0-alpha.1",
    "@betterer/eslint": "^6.0.0-alpha.1",
    "@betterer/typescript": "^6.0.0-alpha.1",
    "@eslint/eslintrc": "^3.3.1",
    "@playwright/test": "^1.54.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@tsconfig/strictest": "^2.0.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.15",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "concurrently": "^9.2.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.23.0",
    "eslint-config-next": "^15.4.2",
    "eslint-formatter-compact": "^8.40.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.7",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "jest-mock-extended": "^4.0.0",
    "jest-silent-reporter": "^0.6.0",
    "lint-staged": "^16.1.2",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "ts-jest": "^29.4.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.37.0",
    "winston": "^3.17.0"
  },
  "ct3aMetadata": {
    "initVersion": "7.39.3"
  },
  "packageManager": "npm@11.3.0",
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,mdx}": [
      "prettier --write"
    ]
  }
}
