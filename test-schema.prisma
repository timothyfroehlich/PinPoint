// Test schema to verify Supabase connection
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_URL_NON_POOLING")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model TestConnection {
  id   Int    @id @default(autoincrement())
  name String
}